{
  "presets": [
    "react",
    [
      "env",
      {
        "targets": {
          "browsers": [
            ">= 1%",
            "ie >= 8"
          ],
          "modules": false,
          "useBuiltIns": true
        }
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": [
        [
          "react-transform",
          {
            "transforms": [
              {
                "transform": "react-transform-hmr",
                "imports": [
                  "react"
                ],
                "locals": [
                  "module"
                ]
              }
            ]
          }
        ]
      ]
    }
  }
}
